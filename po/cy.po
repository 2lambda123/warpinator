msgstr "Mae'r trosglwyddiad i %s wedi gorffen yn llwyddiannus"

#: src/notifications.py:97
#, python-format
msgid "The transfer from %s has finished successfully"
msgstr "Mae'r trosglwyddiad o %s wedi gorffen yn llwyddiannus"

#: src/notifications.py:126 src/warpinator.py:172 src/warpinator.py:175
msgid "Transfer failed"
msgstr "Methodd y trosglwyddiad"

#: src/notifications.py:129
#, python-format
msgid "Something went wrong with the transfer to %s"
msgstr "Aeth rhywbeth o'i le gyda'r trosglwyddiad i %s"

#: src/notifications.py:131
#, python-format
msgid "Something went wrong with the transfer from %s"
msgstr "Aeth rhywbeth o'i le gyda'r trosglwyddiad o %s"

#: src/notifications.py:160 src/warpinator.py:166
msgid "Transfer cancelled"
msgstr "Trosglwyddiad wedi'i ddiddymu"

#: src/notifications.py:163
#, python-format
msgid "Your transfer to %s was cancelled"
msgstr "Cafodd eich trosglwyddiad i %s ei ddiddymu"

#: src/notifications.py:165
#, python-format
msgid "An incoming transfer from %s was cancelled"
msgstr "Cafodd trosglwyddiad i mewn o %s ei ddiddymu"

