msgid "Accept"
msgstr "Derbyn"

#: src/notifications.py:43
msgid "Decline"
msgstr "Gwrthod"

#: src/notifications.py:57
#, python-format
msgid "%(sender_name)s is sending you %(file_count)d file (%(file_name)s)."
msgstr ""
"Mae %(sender_name)s yn anfon %(file_count)d ffeil (%(file_name)s) atoch."

#: src/notifications.py:58
#, python-format
msgid "%(sender_name)s is sending you %(file_count)d files"
msgstr "Mae %(sender_name)s yn anfon %(file_count)d ffeil atoch."

#: src/notifications.py:93
msgid "Transfer complete"
msgstr "Mae'r trosglwyddiad wedi'i gwblhau"

#: src/notifications.py:95
#, python-format
